<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<profiles version="21">
	<profile kind="CodeFormatterProfile" name="Variocube" version="21">
		<!-- Indentation: tabs, width 4 -->
		<setting id="org.eclipse.jdt.core.formatter.tabulation.char" value="tab"/>
