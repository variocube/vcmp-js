{
  "name": "@variocube/vcmp",
  "version": "2.0.1",
  "description": "Variocube Messaging Protocol",
  "main": "cjs/index.js",
  "type": "module",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "browser": {
    "./esm/random.js": "./esm/random-browser.js"
  },
  "scripts": {
    "build": "tsc && tsc -m commonjs --outDir ./cjs",
    "test": "mocha --esm --loader=ts-node/esm --colors tests/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/variocube/vcmp-js.git"
  },
  "author": "Variocube",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/variocube/vcmp-js/issues"
  },
  "homepage": "https://github.com/variocube/vcmp-js#readme",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^20.3.1",
    "@types/ws": "^8.5.4",
    "chai": "^4.3.4",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "ws": "^8.13.0"
  },
  "optionalDependencies": {
    "ws": "^8.13.0",
    "@types/ws": "^8.5.4"
  },
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "default": "./esm/index.js"
    },
    "./server": {
      "types": "./esm/server/index.d.ts",
      "default": "./esm/server/index.js"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "cjs/**/*",
    "esm/**/*",
    "src/**/*"
  ]
}
